{
  "name": "it-stats-revamp-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "pnpm --filter db run generate",
    "db:migrate": "pnpm --filter db run migrate",
    "db:push": "pnpm --filter db run push",
    "db:studio": "pnpm --filter db run studio"
  },
  "devDependencies": {
    "@turbo/gen": "^1.13.3",
    "eslint": "^9.25.1",
    "prettier": "^3.2.5",
    "turbo": "^1.13.3"
  },
  "packageManager": "pnpm@8.15.6"
}
